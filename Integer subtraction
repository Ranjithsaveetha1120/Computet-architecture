#include <stdio.h>

int main() {
    int num1, num2, result;

    // Input two integers
    printf("Enter the first integer: ");
    scanf("%d", &num1);

    printf("Enter the second integer: ");
    scanf("%d", &num2);

    // Subtract the second integer from the first
    result = num1 - num2;

    // Display the result
    printf("Result of subtraction: %d\n", result);

    return 0;
}
